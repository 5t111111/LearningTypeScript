{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-namespace": false,
        "prefer-const": false,
        "no-var-keyword": false,
        "only-arrow-functions": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "no-empty-interface": false,
        "interface-name": [false],
        "max-classes-per-file": [false],
        "variable-name": false,
        "no-console": [false]
    },
    "rulesDirectory": []
}